.ascx_foot_postionview{
  width:100%;
  height:107rpx;
}
.ascx_foot_postion{
  position: fixed;
  z-index: 999;
  background: #ffffff;
  border-top: solid 1px #f7f9fa;
  bottom: 0%;
  height: calc(107rpx + constant(safe-area-inset-bottom));
  height: calc(107rpx + env(safe-area-inset-bottom));
  left: 0%;
  width: 100%;
  align-items: center;
}
.ascx_foot_view{
  display: flex;
  height: 106rpx;
  width: 100%;
  justify-content: center;
  align-items: center;
}
.ascx_foot_normalnav{
  width: 100%;
  margin-top: 12rpx;
  display: flex;
}
.ascx_foot_normalnavdisplay{
  display: none;
}
.ascx_foot_collectionnav{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
}
.ascx_foot_collectionnavdisplay{
  display: none;
}
.ascx_collection_collect{
  width: 93%;
  border: solid 1px #f7f9fa;
  border-radius: 40rpx;
  height: 62rpx;
  line-height: 62rpx;
  font-size: 28rpx;
  letter-spacing: 3rpx;
}
.ascxfootcollectsignup{
  width: 30%;
  letter-spacing: 0rpx;
}
.acsxfootcollected{
  background: #ff6900;
  color: #ffffff;
}
.ascx_collection_signup{
  margin-left: 2%;
  width: 63%;
  border: solid 1px #ff6900;
  color: #ff6900;
  border-radius: 40rpx;
  height: 62rpx;
  line-height: 62rpx;
  font-size: 28rpx;
  letter-spacing: 3rpx;
}
.oderbtnpress{
  border: solid 1px #f7f9fa;
  background: #f7f9fa;
  color: #545454;
}
.collected{
  color: #ffffff;
  background: #ff6900;
}
.ascx_foot_left{
  display: flex;
  text-align: center;
  width: 65%;
  font-size: 12px;
}
.ascx_right_nav{
  width: 33.33%;
}
.ascx_nav_icon{

}
.ascx_nav_iconimg{
  width: 45rpx;
  height: 45rpx;
}
.ascx_nav_title{

}
.select{
  color: #ff6900;
}
.ascx_foot_right{
  width: 35%;
  height: 66rpx;
  margin-top: 6rpx;
}
.ascx_right_consultation{
  width: 220rpx;
  height: 66rpx;
}
.ascx_foot_contactbtn{
  background-color: rgba(255, 255, 255, 0);
  border: 0px;
  height: 66rpx;
  right: 0;
  bottom: 0rpx;
  padding-left: 0rpx;
  padding-right: 0rpx;
}
.ascx_foot_contactbtn::after{
  border: 0px;
}

/* 问答详情用foot */
.ascx_foot_answer_view{
  width: 90%;
  height: 106rpx;
  margin: 0px auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.ascx_foot_answer_backgroun{
  width: 100%;
  height: 76rpx;
  font-size: 18px;
  border-radius: 38rpx;
  text-align: center;
  color: #ffffff;
  line-height: 76rpx;
  letter-spacing: 6rpx;
  background: #ff6900;
}
