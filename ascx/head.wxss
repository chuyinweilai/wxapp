.ascx_head_postionview{
  width:100%;
}
.ascx_head_postionviewNoSlogon{
  width:100%;
}
.ascx_head_postion{
  z-index: 999;
  background: #ffffff;
  width: 100%;
}
.ascx_head_view{
  display: flex;
  height: 73rpx;
}
.ascx_head_left{
  width: 22%;
  display: flex;
  text-align: center;
  margin-left: 2%;
  height: 56rpx;
  border-radius: 30rpx;
  border: solid 1px #f3f3f3;
  align-items: center;
}
.ascx_head_leftview{
  height: 34rpx;
  width: 100%;
  display: flex;
}
.ascx_head_back{
  width: 49%;
  height: 34rpx;
  border-right: solid 1px #f3f3f3;
}
.ascx_head_home{
  width: 49%;
  height: 34rpx;
}
.ascx_head_right{
  width: 54%;
  font-size: 18px;
  font-weight: bold;
  line-height: 56rpx;
  text-align: center;
}
.ascx_head_backimg{
  width: 34rpx;
  height: 34rpx;
}
.ascx_head_homeimg{
  width: 34rpx;
  height: 34rpx;
}
.ascx_head_slogon{
  width: 100%;
  height: 91rpx;
}
.ascx_head_slogonimg{
  width: 100%;
}