.hot_view{
  width: 90%;
  margin: 0px auto;
  margin-top: 35rpx;
  overflow: hidden;
  white-space: nowrap;
  border-bottom: solid 1px #e2e2e2;
}
.hot_title_count{
  font-size: 12px;
  line-height: 60rpx;
}
.hot_event{
  width: auto;
  overflow: hidden;
  margin-bottom: 30rpx;
}
.hot_group{
  margin-right: 40rpx;
  display: inline-block
}
.hot_group_img{
  width: 420rpx;
  height: 183rpx;
  border: solid 13rpx #ffffff;
  border-radius: 8px;
}
