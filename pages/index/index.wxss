  padding-left: 2rpx;
}

.recommend_view{
  width: 90%;
  margin: 0px auto;
  margin-top: 35rpx;
  border-bottom: solid 1px #e2e2e2;
}
.recommend_mainview{
  display: flex;
  flex-wrap: wrap;
  margin-bottom: 50rpx;
}
.recommend_group_left{
  width: 48%;
  background: #ffffff;
  margin-top: 40rpx;
  margin-right: 3%;
  border-radius: 6rpx;
  box-shadow: 0 0 10rpx rgba(191,191,191,0.6);
}
.recommend_group_right{
  width: 48%;
  background: #ffffff;
  margin-top: 40rpx;
  border-radius: 6rpx;
  box-shadow: 0 0 10rpx rgba(191,191,191,0.6);
}
.recommend_ins_logo{
  width: 100%;
  height: 190rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-top-left-radius: 15rpx;
  border-top-right-radius: 15rpx;
  position: relative;
}
.recommend_ins_logoimg{
  width: 100%;
  height: 100%;
  margin-top: 32rpx;
}
.recommend_ins_titleword{
  font-size: 100rpx;
}
.recommend_ins_content{
  padding-left: 3%;
  padding-right: 3%;
  padding-top: 10rpx;
  padding-bottom: 30rpx;
  font-size: 24rpx;
  line-height: 46rpx;
  text-align: center;
  background: #ffffff;
  height: 150rpx;
  border-bottom-left-radius: 15rpx;
  border-bottom-right-radius: 15rpx;
}
.recommend_title{
  font-size: 32rpx;
  font-weight: bold;
  line-height: 55rpx;
  text-align: center;
}
.recommend_type{
  color: #999999;
  font-size: 24rpx;
  line-height: 32rpx;
  white-space: nowrap;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  -ms-text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
  height: 32rpx;
}
.recommend_ins_fee{
  margin-top: 20rpx;
}
.recommend_fee{
  color: #ff6900;
}
.recommend_fee_number{
  font-weight: bold;
  font-size:36rpx;
}
.recommend_intro{
  color: #545454;
  line-height: 35rpx;
  font-size: 14px;
}
.recommend_verification{
  position: absolute;
  width: 150rpx;
  height: 16px;
  left: 0rpx;
  top: 0rpx;
}

