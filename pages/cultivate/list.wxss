/* pages/cultivate/list.wxss */
.cultivate_view{

}
.cultivate_head{
  background: #ffffff;
  height: 200rpx;
}
.cultivate_head_search{
  height: 130rpx;
  display: flex;
  align-items: center;
}
.cultivate_search_view{
  height: 70rpx;
  width: 90%;
  margin: 0px auto;
  background: #f2f2f2;
  border-radius: 50rpx;
  display: flex;
  align-items: center;
}
.cultivate_search_default{
  width: 100%;
  margin: 0px auto;
  height: 40rpx;
  display: flex;
}
.cultivate_search_defaultimg{
  width: 36rpx;
  height: 36rpx;
}
.cultivate_search_default_img{
  width: 50%;
  text-align: right;
}
.word_search{
  font-size: 18px;
  color: #aaaaaa;
  line-height: 40rpx;
  padding-left: 1%;
  width: 50%;
}
.newIconSearch{
  width: auto;
  text-align: left;
  margin-left: 30rpx;
}
.cultivate_sreen_iconimg{
  width: 20rpx;
  height: 20rpx;
  margin-left: 6rpx;
}
.cultivate_head_screen{
  display: flex;
}
.cultivate_screen_title{
  width: 50%;
  text-align: center;
  font-size: 16px;
}
.cultivate_list{
  width: 100%;
  background: #f8f9fa;
}
.cultivate_screen_view{
  z-index: 10;
  background: #ffffff;
  width:100%;
  position: absolute;
}
.cultivate_screen_condition{
  width: 92%;
  margin: 0px auto;
  margin-bottom: 10rpx;
  border-top: solid 1px #f8f9fa;
  border-bottom: solid 1px #f8f9fa;
}
.cultivate_list_groupview{
  width: 100%;
  height: 804rpx;
  padding-bottom: 20rpx;
  background: #f8f9fa;
}
.cultivate_screen_block{
  width:100%;
  height: 824rpx;
  position:fixed;
  z-index:2;
  background:#a3a3a3;
  opacity:0.6;
}
.cultivate_condition_group{
  display: flex;
  font-size: 16px;
  margin: 30rpx 0rpx;
}
.cultivate_condition_groupdisplay{
  display: none;
}
.cultivate_condition_title{
  width: 120rpx;
  height: 56rpx;
  margin-right: 25rpx;
  text-align: center;
  line-height: 56rpx;
  border-radius: 28rpx;
  color: #545454;
  border: solid 1px #f8f9fa;
}
.cultivate_condition_title3{
  width: 155rpx;
  height: 56rpx;
  margin-right: 25rpx;
  text-align: center;
  line-height: 56rpx;
  border-radius: 28rpx;
  color: #545454;
  border: solid 1px #f8f9fa;
}
.cultivate_condition_feegroup{
  width: 80%;
  margin: 0px auto;
  display: flex;
  color: #545454;
}
.cultivate_condition_fee{
  text-align: center;
  height: 56rpx;
  line-height: 56rpx;
  border: solid 1px #f8f9fa;
  width: 33.33%;
}
.cultivate_condition_fee2{
  text-align: center;
  height: 56rpx;
  line-height: 56rpx;
  width: 33.33%;
}
.screenselect{
  color: #ffffff;
  border: solid 1px #ff6900;
  background: #ff6900;
}
.cultivate_condition_bottom{
  width: 92%;
  margin: 0px auto;
}
.cultivate_condition_btnview{
  display: flex;
}
.cultivate_condition_btn{
  width: 50%;
  text-align: center;
  font-size: 20px;
  height: 80rpx;
  line-height: 76rpx;
}
.cultivate_list_gourp{
  margin-top: 50rpx;
}
.cultivate_group{
  display: flex;
  margin-top: 40rpx;
}
.cultivate_group_logo{
  width: 280rpx;
  height: 220rpx;
  margin-right: 25rpx;
  border-radius: 10rpx;
  border: solid 2px #ebebec;
  overflow: hidden;
}
.cultivate_group_logoimg{
  width: 280rpx;
  height: 220rpx;
}
.cultivate_group_right{
  width: 370rpx;
  line-height: 70rpx;
}
.cultivate_group_title{
  font-weight: bold;
  height: 70rpx;
  font-size: 16px;
  display: flex;
}
.cultivate_group_titleview{
  width: 250rpx;
}
.cultivate_group_newscore{
  font-size: 8px;
  padding: 2rpx;
  padding-left: 6rpx;
  height: 30rpx;
  line-height: 30rpx;
  border-radius: 15rpx;
  background: #ff6900;
  color: #ffffff;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
}
.cultivate_group_newscoredisplay{
  display: none;
}
.cultivate_group_intro{
  font-size: 14px;
  line-height: 45rpx;
  color: #545454;
}
.cultivate_group_fee{
  font-size: 14px;
  color: #545454;
}
.cultivate_fee_number{
  font-size: 16px;
  color: #ff6900;
}
.cultivate_group{
  display: flex;
  flex-wrap: wrap;
  width: 92%;
  margin: 0px auto;
  margin-bottom: 30rpx;
}
@import "../../wxss/IphoneX.wxss";