/* pages/cultivate/detail.wxss */
@import "ascx/detail_teacherlist";
@import "ascx/detail_course";
@import "ascx/detail_topscore";
@import "ascx/culfoot";

.cul_detail_view{
  
}
.cul_detail_head{
  width: 100%;
  height: 300rpx;
  background-size: 100% 300rpx;
}
.defaultbanner{
  background-image: url('https://testwx.taoraise.com/UploadFiles/cultivate/design/bg.jpg');
}
.cul_detail_bannerimg{
  width: 100%;
  height: 300rpx;
}
.cul_detail_intro{
  width: 90%;
  margin: 0px auto;
  margin-top: 10rpx;
  font-size: 16px;
}
.cul_intro_title{
  font-size: 35rpx;
  font-weight: bold;
  line-height: 60rpx;
}
.cul_intro_details{
  line-height: 44rpx;
}
.cul_intro_address{
  display: flex;
}
.cul_address_showmore{
  width: 100%;
  font-size: 14px;
  line-height: 60rpx;
  height: 60rpx;
  text-align: center;
  color: #545454;
}
.cul_address_showmoreimg{
  width: 20rpx;
  height: 20rpx;
  margin-left: 6rpx;
}
.cul_address_title{
  width: 200rpx;
}
.cul_address_detail{
  width: 465rpx;
  margin-left: 10rpx;
}
.cul_address_group{
  margin-bottom: 10rpx;
}
.cul_head_intro{
  padding-top: 150rpx;
  width: 90%;
  margin: 0px auto;
  color: #ffffff;
}
.cul_head_title{
  font-weight: bold;
}
.cul_head_slogon{
  font-size: 14px;
  line-height: 40rpx;
}
.cul_head_other{
  font-size: 14px;
  line-height: 50rpx;
}
.word_heighlight{
  font-size: 18px;
  font-weight: bold;
}
.cul_detail_nav{
  margin-top: 20rpx;
  font-size: 16px;
}
.cul_nav_title{
  display: flex;
  height: 50rpx;
}
.cul_title_view{
  width: 33.33%;
  text-align: center;
}
.cul_title_bottom{
  display: flex;
  height: 24rpx;
  border-bottom: solid 1px #f8f9fa;
}
.cul_title_bottomline{
  width: 33.33%;
  text-align: center;
}
.cul_bottomline_view{
  width: 25%;
  margin: 0px auto;
  border-bottom: solid 2px #ff6900;
}
.cul_detail_content{
  
}

.cul_detail_viewbreak{
  width: 100%;
  height: 20rpx;
  background: #f8f9fa;
}
.cul_detail_viewbreak_bottom{
  width: 100%;
  height: 30rpx;
  background: #f8f9fa;
}